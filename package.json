  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/base/issues"
  },
  "homepage": "https://github.com/vigour-io/base#readme",
  "contributors": [
    "Jim de Beer <jim@vigour.io>"
  ]
}
