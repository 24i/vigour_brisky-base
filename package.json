{
  "name": "vigour-base",
  "version": "3.3.4",
  "description": "'injectacble classes'",
  "main": "lib/index.js",
  "scripts": {
    "test": "node test | tap-difflet && standard",
    "watch": "nodemon test | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "docs": "node_modules/vigour-doc/bin/vdoc",
    "perf-browser": "budo ./test/performance/index.js -p 8080 --live",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/base.git"
  },
  "keywords": [
    "class",
    "prototype",
    "base"
  ],
  "dependencies": {
    "vigour-util": "^3.0.3",
    "lodash.merge": "^4.4.0",
    "vigour-stamp": "^1.1.1",
    "babelify": "^7.3.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "standard": "^7.0.1",
    "tap-difflet": "0.4.0",
    "tape": "4.4.0",
    "vigour-performance": "^1.3.1",
    "nodemon": "^1.9.1",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "vigour-doc": "^1.0.5",
    "budo": "^8.2.2",
    "human-readable-ids": "^1.0.1",
    "pre-commit": "^1.1.3"
  },
  "browser": {
    "./lib/iterator/index.js": "./lib/iterator/browser.js"
  },
